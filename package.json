    "@mui/system": "^5.16.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^4.12.0",
    "react-transition-group": "^4.4.5",
    "utility-types": "^3.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
