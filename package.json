    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-storybook": "^0.6.4",
    "html-webpack-plugin": "^5.5.0",
    "lint-staged": "~10.5.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "prettier-stylelint": "^0.4.2",
    "simple-git-hooks": "^2.6.1",
    "style-dictionary": "^3.7.1",
    "token-transformer": "^0.0.27",
    "typescript": "^4.8.3",
    "vite": "^3.1.0"
  }
